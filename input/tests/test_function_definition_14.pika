func hello <[rat] a> -> void {
	var i := 0.
	while (i < length a) {
		print a[i], _n_.
		i := i + 1.
	}
	return.
}

func hello2 <[[rat]] a> -> void {
	var i := 0.
	while (i < length a) {
		print a[i], _n_.
		i := i + 1.
	}
	return.
}

exec {
	var a := [1 // 2, 3 // 4, 5 // 6, 7 // 8].
	call hello(a).
}
