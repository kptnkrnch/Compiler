exec {
	var numbers := [1, 3, 5, 8, 11, 12].
	var numbers2 := [5, 7, 3, 17, 13, 6].
	
	var max := <int a, int b> -> int {
		if (a > b) {
			return a.
		} else {
			return b.
		}
	}.
	var result := zip numbers, numbers2, max. 
	var result2 := reverse zip reverse numbers, reverse numbers2, max. 
	print result,_n_.
	print result2,_n_.
}
